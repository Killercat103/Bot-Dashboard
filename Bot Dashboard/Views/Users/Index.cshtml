@{
    ViewData["Title"] = "Account";
}
<div class="maincolumn">
    <h1>@((string?)ViewData["Username"] + '#' + (string?)ViewData["User_Discriminator"])</h1>
    <img src="@("https://cdn.discordapp.com/avatars/" +
					ViewData["User_ID"] +
					'/' + ViewData["User_Avatar"])"
                    height="128"
                    width="128"
                    alt="profile picture" />
</div>